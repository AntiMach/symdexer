{
  "name": "symdexer",
  "displayName": "symdexer",
  "description": "A python symbol indexer",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "symdexer.init",
        "title": "Symdexer: Initialize"
      },
      {
        "command": "symdexer.cache",
        "title": "Symdexer: Cache index"
      },
      {
        "command": "symdexer.find",
        "title": "Symdexer: Find symbol"
      }
    ],
    "configuration": [
      {
        "title": "Symdexer",
        "properties": {
          "symdexer.configFile": {
            "type": "string",
            "default": "symdexer.toml",
            "description": "Default configuration file"
          }
        }
      }
    ],
    "configurationDefaults": {}
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}